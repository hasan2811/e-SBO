
rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    // ATURAN TERBUKA: Izinkan semua operasi baca dan tulis untuk semua file.
    // Ini menghilangkan semua batasan keamanan untuk fokus pada fungsionalitas.
    match /{allPaths=**} {
      allow read, write: if true;
    }
  }
}
