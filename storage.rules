rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    // PERINGATAN: Aturan ini sangat longgar dan hanya untuk tujuan pengembangan.
    // Aturan ini mengizinkan semua pengguna yang sudah login untuk mengunggah,
    // membaca, dan menghapus file apa pun di Storage.
    match /{allPaths=**} {
      allow read, write: if request.auth != null;
    }
  }
}
