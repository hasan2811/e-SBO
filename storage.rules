rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {

    // WARNING: These rules are intentionally permissive for debugging purposes.
    // Any authenticated user can read and write ANY file.
    // This is NOT secure for a production environment.

    match /{allPaths=**} {
      allow read, write: if request.auth != null;
    }
  }
}
