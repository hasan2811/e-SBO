
rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // Aturan ini mengizinkan semua pengguna yang sudah login (terautentikasi)
    // untuk mengunggah (write) dan mengunduh (read) file apa pun.
    match /{allPaths=**} {
      allow read, write: if request.auth != null;
    }
  }
}
