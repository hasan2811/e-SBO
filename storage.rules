
rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    // WARNING: This rule is completely public and allows anyone to upload, read, or delete any file.
    // This is NOT recommended for production environments.
    // This is set based on the user's explicit request to remove all server-side security.
    match /{allPaths=**} {
      allow read, write: if true;
    }
  }
}
