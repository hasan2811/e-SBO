
rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    // This rule allows any authenticated user to read and write any file.
    // This is for development and troubleshooting ONLY.
    // DO NOT use this in a production environment without securing your data.
    match /{allPaths=**} {
      allow read, write: if request.auth != null;
    }
  }
}
