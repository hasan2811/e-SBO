rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    // WARNING: These rules are completely open for debugging purposes.
    // They allow anyone to read and write to your storage bucket.
    // Re-secure these rules once the application logic is confirmed to be working.
    match /{allPaths=**} {
      allow read, write: if true;
    }
  }
}
