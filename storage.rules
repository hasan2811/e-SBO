rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    // Aturan untuk Storage
    // Aturan ini mengizinkan pengguna yang sudah terautentikasi untuk melakukan semua operasi:
    // get (melihat/mengunduh), list (melihat daftar), create (mengunggah), update, dan delete.
    match /{allPaths=**} {
      allow get, list, create, update, delete: if request.auth != null;
    }
  }
}
