rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // WARNING: This rule allows any client to upload, download, and delete files.
    // This is set to the absolute minimum as requested to resolve permission issues,
    // trusting all security logic to the front-end.
    match /{allPaths=**} {
      allow read, write: if true;
    }
  }
}
