
rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    // Aturan paling mendasar: Izinkan baca dan tulis jika pengguna sudah login.
    // Ini memastikan tidak ada masalah izin saat mengunggah file.
    match /{allPaths=**} {
      allow read, write: if request.auth != null;
    }
  }
}
