rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    
    // WARNING: These rules are intentionally permissive for debugging purposes.
    // Any authenticated user can read and write ANY data.
    // This is NOT secure for a production environment.
    
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}
